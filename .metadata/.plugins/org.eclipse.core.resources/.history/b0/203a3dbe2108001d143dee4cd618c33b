package com.array;

public class TwoDimArray {

	public static void main(String[] args) {
		
		int A2D[][] = new int[2][3];//0-2
		A2D[0][0]=0;
		A2D[0][1]=1;
		A2D[0][2]=2;
		A2D[1][0]=3;
		A2D[1][1]=4;
		A2D[1][2]=5;
		A2D[2][0]=6;
		A2D[2][1]=7;
		A2D[2][2]=8;
		
		/// int[][] A2D = {{0,1,2},{3,4,5},{6,7,8}};
		
		int length = A2D.length;
		System.out.println(length);
		
//		for (int i = 0; i < A2D.length; i++) {
//			
//		}
		
		

	}

}
